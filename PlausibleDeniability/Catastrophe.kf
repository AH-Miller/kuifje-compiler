N = 2; // Set N to number of participants
delta = 0.1; // Set 0<=delta<=1
r <- 0 [delta] 1;
i = 0;
Resps = [];
count = 0;
while i < N:
  resp <- uniform [0, 1];
  Resps.append(resp);
  coin <- uniform [0, 1];
  count <- uniform [count + coin, count + resp];
  i = i + 1;
if r == 0:
  count = Resps;
leak(count);