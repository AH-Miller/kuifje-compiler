def calculateForT(t):
  ds <- uniform [1 .. 8];
  i = 0;
  while (i <= t):
    leak(ds == i);
    i = i + 1;
  return ds;

ts1 = calculateForT(1);
ts2 = calculateForT(2);
ts3 = calculateForT(3);
ts4 = calculateForT(4);
