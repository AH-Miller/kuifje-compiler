def calculateForT(t):
  ds <- uniform [1 .. 8];
  i = 0;
  while (i <= t):
    leak(ds == i);
    i = i + 1;
  return ds;

t1 = calculateForT(1);
t2 = calculateForT(2);
t3 = calculateForT(3);
t4 = calculateForT(4);

ts1 = t1;
ts2 = t2;
ts3 = t3;
ts4 = t4;
